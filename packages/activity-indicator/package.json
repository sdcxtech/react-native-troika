{
	"name": "@sdcx/activity-indicator",
	"description": "A native activity indicator for React Native",
	"version": "1.0.1",
	"main": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"react-native": "src/index",
	"nativePackage": true,
	"files": [
		"src",
		"dist",
		"android",
		"common",
		"react-native.config.js",
		"!android/build",
		"!**/__tests__"
	],
	"repository": "https://github.com/listenzz/react-native-troika",
	"homepage": "https://github.com/listenzz/react-native-troika/tree/master/packages/activity-indicator#readme",
	"author": "sdcx",
	"license": "MIT",
	"keywords": [
		"react-native",
		"activity-indicator"
	],
	"scripts": {
		"build": "rm -rf ./dist && tsc -p tsconfig.json",
		"prepare": "npm run build"
	},
	"peerDependencies": {
		"react": ">=16.8",
		"react-native": ">=0.60"
	},
	"devDependencies": {},
	"codegenConfig": {
		"name": "activityindicator",
		"type": "components",
		"jsSrcsDir": "src",
		"android": {
			"javaPackageName": "com.reactnative.activityindicator"
		},
		"ios": null
	}
}
